{
  "name": "ArmoredServices",
  "author": "",
  "keywords": [],
  "version": "1.0.0",
  "description": "Armored Services Front End",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/plugin-syntax-jsx": "latest",
    "@nuxtjs/axios": "5.3.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "cross-env": "5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-vue": "next",
    "jest": "latest",
    "jest-serializer-vue": "^2.0.2",
    "nuxt": "3.12.4",
    "nuxt-class-component": "1.2.1",
    "nuxt-property-decorator": "1.2.0",
    "prettier": "^1.14.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^2.6.0",
    "vuetify": "1.3.2"
  },
  "devDependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "nodemon": "^1.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
